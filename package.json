{
  "name": "webcardinal-app-tester",
  "version": "1.0.0",
  "description": "Application made only for testing purposes.",
  "scripts": {
    "dev": "node node_modules/octopus/scripts/run dev devmode",
    "build": "node node_modules/octopus/scripts/run build devmode",
    "test": "node node_modules/octopus/scripts/run test devmode",
    "create-test": "node node_modules/octopus/scripts/run create-local-test devmode",
    "commit-test": "node node_modules/octopus/scripts/run commit-local-test devmode",
    "clean": "rm -rf .dev/ themes/ webcardinal/",
    "local": "node node_modules/octopus/scripts/run dependencies devmode && npm run build",
    "update": "npm run clean && npm run local",
    "server": "node server.js"
  },
  "devDependencies": {
    "jsdom": "^19.0.0",
    "live-server": "^1.1.0",
    "octopus": "https://github.com/webcardinal/octopus"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webcardinal/webcardinal-app-tester.git"
  },
  "license": "MIT"
}
